{
  "$schema": "https://usearctic.sh/theme.json",
  "defs": {
    "lavenderPurple": "#c8b6ff",
    "softPink": "#ffc6ff",
    "pastelBlue": "#a0c4ff",
    "mintGreen": "#9eeab0",
    "peachOrange": "#ffb380",
    "lilac": "#d4a5d8",
    "babyBlue": "#bde0fe",
    "coral": "#ff8a8a",
    "bgDark": "#1e1e2e",
    "bgPanel": "#252535",
    "bgElement": "#2d2d40"
  },
  "theme": {
    "primary": {
      "dark": "lavenderPurple",
      "light": "#9f86e0"
    },
    "secondary": {
      "dark": "softPink",
      "light": "#e0a0e0"
    },
    "accent": {
      "dark": "pastelBlue",
      "light": "#7aa8e0"
    },
    "error": {
      "dark": "coral",
      "light": "#e06060"
    },
    "warning": {
      "dark": "peachOrange",
      "light": "#e09560"
    },
    "success": {
      "dark": "mintGreen",
      "light": "#70c080"
    },
    "info": {
      "dark": "babyBlue",
      "light": "#90c0e0"
    },
    "text": {
      "dark": "#ffffff",
      "light": "#2b2d42"
    },
    "textMuted": {
      "dark": "#c0c0d0",
      "light": "#6a6a80"
    },
    "background": {
      "dark": "none",
      "light": "none"
    },
    "backgroundPanel": {
      "dark": "bgPanel",
      "light": "#f0f0f8"
    },
    "backgroundElement": {
      "dark": "rgba(200, 182, 255, 0.08)",
      "light": "rgba(100, 80, 160, 0.08)"
    },
    "backgroundMenu": {
      "dark": "bgElement",
      "light": "#e8e8f0"
    },
    "border": {
      "dark": "lavenderPurple",
      "light": "#9f86e0"
    },
    "borderActive": {
      "dark": "softPink",
      "light": "#e0a0e0"
    },
    "borderSubtle": {
      "dark": "#404058",
      "light": "#d0d0e0"
    },
    "diffAdded": {
      "dark": "mintGreen",
      "light": "#60a070"
    },
    "diffRemoved": {
      "dark": "coral",
      "light": "#d06060"
    },
    "diffContext": {
      "dark": "#c0c0d0",
      "light": "#6a6a80"
    },
    "diffHunkHeader": {
      "dark": "lavenderPurple",
      "light": "#9f86e0"
    },
    "diffHighlightAdded": {
      "dark": "#b0f0c0",
      "light": "#70b080"
    },
    "diffHighlightRemoved": {
      "dark": "#ffa0a0",
      "light": "#e07070"
    },
    "diffAddedBg": {
      "dark": "rgba(158, 234, 176, 0.15)",
      "light": "rgba(96, 160, 112, 0.2)"
    },
    "diffRemovedBg": {
      "dark": "rgba(255, 138, 138, 0.15)",
      "light": "rgba(208, 96, 96, 0.2)"
    },
    "diffContextBg": {
      "dark": "none",
      "light": "none"
    },
    "diffLineNumber": {
      "dark": "#c0c0d0",
      "light": "#6a6a80"
    },
    "diffAddedLineNumberBg": {
      "dark": "rgba(158, 234, 176, 0.2)",
      "light": "rgba(96, 160, 112, 0.25)"
    },
    "diffRemovedLineNumberBg": {
      "dark": "rgba(255, 138, 138, 0.2)",
      "light": "rgba(208, 96, 96, 0.25)"
    },
    "markdownText": {
      "dark": "#ffffff",
      "light": "#2b2d42"
    },
    "markdownHeading": {
      "dark": "lavenderPurple",
      "light": "#9f86e0"
    },
    "markdownLink": {
      "dark": "pastelBlue",
      "light": "#7aa8e0"
    },
    "markdownLinkText": {
      "dark": "softPink",
      "light": "#e0a0e0"
    },
    "markdownCode": {
      "dark": "mintGreen",
      "light": "#60a070"
    },
    "markdownBlockQuote": {
      "dark": "#c0c0d0",
      "light": "#6a6a80"
    },
    "markdownEmph": {
      "dark": "softPink",
      "light": "#e0a0e0"
    },
    "markdownStrong": {
      "dark": "lavenderPurple",
      "light": "#9f86e0"
    },
    "markdownHorizontalRule": {
      "dark": "lilac",
      "light": "#b080b8"
    },
    "markdownListItem": {
      "dark": "lavenderPurple",
      "light": "#9f86e0"
    },
    "markdownListEnumeration": {
      "dark": "pastelBlue",
      "light": "#7aa8e0"
    },
    "markdownImage": {
      "dark": "pastelBlue",
      "light": "#7aa8e0"
    },
    "markdownImageText": {
      "dark": "softPink",
      "light": "#e0a0e0"
    },
    "markdownCodeBlock": {
      "dark": "#ffffff",
      "light": "#2b2d42"
    },
    "syntaxComment": {
      "dark": "#9090a8",
      "light": "#8080a0"
    },
    "syntaxKeyword": {
      "dark": "lavenderPurple",
      "light": "#9f86e0"
    },
    "syntaxFunction": {
      "dark": "pastelBlue",
      "light": "#7aa8e0"
    },
    "syntaxVariable": {
      "dark": "softPink",
      "light": "#e0a0e0"
    },
    "syntaxString": {
      "dark": "mintGreen",
      "light": "#60a070"
    },
    "syntaxNumber": {
      "dark": "peachOrange",
      "light": "#d08050"
    },
    "syntaxType": {
      "dark": "babyBlue",
      "light": "#90c0e0"
    },
    "syntaxOperator": {
      "dark": "#d0d0e0",
      "light": "#6a6a80"
    },
    "syntaxPunctuation": {
      "dark": "#e0e0f0",
      "light": "#404058"
    }
  }
}
